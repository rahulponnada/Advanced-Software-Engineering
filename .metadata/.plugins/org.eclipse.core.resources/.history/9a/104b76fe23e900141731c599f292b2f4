<script>
function Search(query){
	alert(query);
	window.location = '/yotube/searchpage?query='+query;
	document.getElementById("search").value=query;
}
function searching(){
	var query= document.getElementById("search").value;
	alert(query);
	window.location = '/yotube/searchpage?query='+query;
}
</script>

